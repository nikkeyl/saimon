{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ignorePatterns": [
        "inputMask.js",
        "map.js"
    ],
    "rules": {
        "no-inner-declarations": 0,
        "no-useless-escape": 0,
        "no-console": 0
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    }
}